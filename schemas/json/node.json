{
  "$id": "@ppln/schemas/node.json",
  "additionalProperties": false,
  "type": "object",
  "properties": {
    "_id": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "taskId": {
      "type": "string"
    },
    "inputs": {
      "type": "array",
      "items": {
        "additionalProperties": false,
        "type": "object",
        "properties": {
          "nodeId": {
            "type": "string"
          },
          "outputChannel": {
            "type": "string"
          },
          "inputChannel": {
            "type": "string"
          }
        },
        "required": [
          "nodeId",
          "outputChannel",
          "inputChannel"
        ]
      }
    },
    "numExecutions": {
      "minimum": 1,
      "default": 1,
      "type": "integer"
    },
    "params": {
      "type": "object",
      "patternProperties": {
        "^.*$": {}
      }
    },
    "workerId": {
      "type": "string"
    },
    "position": {
      "additionalProperties": false,
      "type": "object",
      "properties": {
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        }
      },
      "required": [
        "x",
        "y"
      ]
    }
  },
  "required": [
    "_id",
    "createdAt",
    "taskId",
    "inputs",
    "position"
  ]
}